<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DriverController">
   <center>
      <Pane prefHeight="400.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="logoutButton" layoutX="347.0" layoutY="1.0" mnemonicParsing="false" onAction="#handleLogoutButton" text="logout" textFill="#bf221a" />
            <Button fx:id="BasicInformationButton" layoutX="117.0" layoutY="99.0" mnemonicParsing="false" onAction="#handleBasic" prefHeight="64.0" prefWidth="190.0" text="Basic Information">
               <font>
                  <Font size="20.0" />
               </font>
               <effect>
                  <InnerShadow choke="0.27" color="#bec923" height="71.4" radius="35.8825" width="74.13" />
               </effect>
            </Button>
            <Button fx:id="assignedTripsButton" layoutX="117.0" layoutY="190.0" mnemonicParsing="false" onAction="#handleAssignedTrips" prefHeight="64.0" prefWidth="190.0" text="Assigned Trips">
               <font>
                  <Font size="20.0" />
               </font>
               <effect>
                  <InnerShadow choke="0.27" color="#bec923" height="71.4" radius="35.8825" width="74.13" />
               </effect>
            </Button>
         </children>
      </Pane>
   </center>
</BorderPane>