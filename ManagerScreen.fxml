<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ManagerController">
   <center>
      <Pane prefHeight="400.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="viewTripsButton" layoutX="109.0" layoutY="96.0" mnemonicParsing="false" onAction="#handleViewTrips" prefHeight="44.0" prefWidth="183.0" text="view trips">
               <font>
                  <Font size="20.0" />
               </font>
               <effect>
                  <InnerShadow blurType="GAUSSIAN" choke="0.27" color="#306aa4" height="66.31" radius="33.1875" width="68.44" />
               </effect>
               <cursor>
                  <Cursor fx:constant="E_RESIZE" />
               </cursor>
            </Button>
            <Button fx:id="addTripsButton" layoutX="109.0" layoutY="155.0" mnemonicParsing="false" onAction="#handleAddTrips" prefHeight="44.0" prefWidth="183.0" text="add trips">
               <font>
                  <Font size="20.0" />
               </font>
               <effect>
                  <InnerShadow choke="0.27" color="#306aa4" height="59.92" radius="31.590000000000003" width="68.44" />
               </effect>
            </Button>
            <Button fx:id="removeTripsButton" layoutX="111.0" layoutY="212.0" mnemonicParsing="false" onAction="#handleRemoveTrips" prefHeight="44.0" prefWidth="183.0" text="remove trips">
               <font>
                  <Font size="20.0" />
               </font>
               <effect>
                  <InnerShadow choke="0.27" color="#306aa4" height="59.92" radius="31.590000000000003" width="68.44" />
               </effect>
            </Button>
            <Button fx:id="assignButton" layoutX="112.0" layoutY="269.0" mnemonicParsing="false" onAction="#handleAssignButton" prefHeight="44.0" prefWidth="183.0" text="assign drivers">
               <font>
                  <Font size="20.0" />
               </font>
               <effect>
                  <InnerShadow choke="0.27" color="#306aa4" height="59.92" radius="31.590000000000003" width="68.44" />
               </effect>
            </Button>
            <Button fx:id="logoutButton" layoutX="348.0" mnemonicParsing="false" onAction="#handleLogoutButton" prefHeight="25.0" prefWidth="52.0" text="logout" textFill="#961212" />
         </children>
      </Pane>
   </center>
</BorderPane>